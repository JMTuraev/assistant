generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    Int     @id @unique @default(autoincrement())
  email String  @unique
  name  String?
  first  String?
  phone  String
}

model Role {

  id    Int     @id @unique @default(autoincrement())
  name String
  level Int

}